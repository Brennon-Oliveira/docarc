#!/usr/bin/env bash

if [ -f ./docaGit/.gitrc ]; then
	source ./docaGit/.gitrc
fi

DocaRed='\033[0;31m'
DocaGreen='\033[0;32m'
DocaBlue='\033[0;34m'
DocaPurple='\033[0;35m'
DocaYellow='\033[1;33m'
DocaLigthRed='\033[1;31m'
DocaLigthBlue='\033[1;34m'
DocaWhite='\033[1;37m'

doca(){
	if [ $1 = "git" ]; then
		docaGit $*
	fi
}